{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "affreviews/single",
	"version": "0.1.1",
	"title": "Single Review",
	"category": "affreviews",
	"icon": "star-filled",
	"keywords": ["single", "affiliates", "review"],
	"description": "Display a single review inside reviews or pages.",
	"supports": {
		"html": false
	},
	"attributes": {
		"selectedPost": {
			"type": "integer",
			"default": 0
		},
		"buttonText": {
			"type": "string",
			"default": "Check Best Deals"
		}
	},
	"textdomain": "affreviews",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
